{
  "name": "port",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build",
    "build:lib": "ng build hal-utils",
    "watch": "ng build hal-utils --watch --configuration development",
    "watch:lib": "ng build --watch --configuration development",
    "test": "npm run test:lib && npm run build:lib && npm run test:app",
    "test:lib": "jest --config ./jest.lib.config.js",
    "test:app": "jest --config ./jest.app.config.js",
    "e2e:open": "npx ng run portfolio:cypress-open",
    "e2e:run": "npx ng run portfolio:cypress-run",
    "lint": "ng lint",
    "lint:all": "npm run lint-css && npm run lint",
    "lint-css": "stylelint src/**/*.scss",
    "prettier:fix": "npm run prettier:fix:ts && npm run prettier:fix:scss && npm run prettier:fix:html",
    "prettier:fix:ts": "prettier --config \".prettierrc.json\" --write \"src/**/*.ts\"",
    "prettier:fix:scss": "prettier --config \".prettierrc.json\" --write \"src/**/*.scss\"",
    "prettier:fix:html": "prettier --config \".prettierrc.json\" --write \"src/**/*.html\"",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/portfolio/stats.json",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && typecheck && lint:all"
    }
  },
  "lint-staged": {
    "*.{js,ts,html,md,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/router": "~13.0.3",
    "@ng-web-apis/common": "^1.12.1",
    "@ngneat/until-destroy": "^8.0.4",
    "lodash-es": "^4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@briebug/cypress-schematic": "^5.0.0",
    "@ngneat/spectator": "^7.1.0",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "autoprefixer": "^10.2.6",
    "cypress": "7.5.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-rxjs": "^3.3.3",
    "eslint-plugin-rxjs-angular": "^1.0.6",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-jasmine2": "^27.0.4",
    "jest-preset-angular": "^9.0.3",
    "ng-mocks": "^12.1.0",
    "ng-packagr": "^13.0.8",
    "ng-samurai": "^2.0.6",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "prettier-plugin-organize-attributes": "0.0.4",
    "pretty-quick": "^3.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^4.0.0",
    "tailwindcss": "^2.1.4",
    "typescript": "~4.4.4",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
